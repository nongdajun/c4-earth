// 将设置放入此文件中以覆盖默认值和用户设置。
{

    // C4ISR Earth JS IDE configuration

    // Specifies the host name for connect to C4ISR earth server.
    //C4ISR EARTH 服务器控制端口，默认端口50372 (0xc4c4)
    "c4earth.serverHost": null,

    // Specifies the port number for connect to C4ISR earth server.
    //C4ISR EARTH 服务器地址
    "c4earth.serverPort": 50372,

    // Specifies the password for login C4ISR earth server.
    //C4ISR EARTH 服务器登录认证密码
    //支持两种配置：
    //1、配置登录密码的md5值
    //2、直接配置明文密码 "c4earth_password": "md5(明文密码)" 
    "c4earth.serverPassword": null

}